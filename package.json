{
  "name": "vendure-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "start": "concurrently yarn:run:*",
    "build-migrations": "tsc -p tsconfig.migrations.json",
    "build": "rimraf dist/migrations && rimraf dist/src && tsc",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "update-vendure": "yarn add @vendure/admin-ui-plugin @vendure/elasticsearch-plugin @vendure/admin-ui @vendure/asset-server-plugin @vendure/core @vendure/email-plugin @vendure/ui-devkit",
    "test": "echo 'sometime in the future'",
    "deploy": "yarn build && cp -r dist/migrations/*.js migrations/ && rm migrations/*.ts",
    "eslint": "eslint ./src",
    "docs": "typedoc src/"
  },
  "resolutions": {
    "graphql": "16.12.0"
  },
  "dependencies": {
    "@nestjs/apollo": "~13.1.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/core": "^11.0.12",
    "@nestjs/graphql": "~13.1.0",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vendure/admin-ui": "^3.5.1",
    "@vendure/admin-ui-plugin": "^3.5.1",
    "@vendure/asset-server-plugin": "^3.5.1",
    "@vendure/core": "^3.5.1",
    "@vendure/elasticsearch-plugin": "^3.5.1",
    "@vendure/email-plugin": "^3.5.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "16.12.0",
    "graphql-tag": "^2.12.6",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.7",
    "prettier": "^2.8.0",
    "rimraf": "^6.1.2",
    "typedoc": "^0.28.15",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@vendure/migrate-v2": "^0.4.0",
    "concurrently": "^5.3.0",
    "ts-node": "^10.9.2"
  },
  "packageManager": "yarn@4.12.0"
}
